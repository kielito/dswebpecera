<table border="1">
    <theader>
        <th>Id</th>
        <th>Nombre</th>
        <th>Password</th>
        <th>Accion</th>
    </theader>
    {{#each users}}
    <tr>
        <td>{{Id}}</td>
        <td>{{Usuario}}</td>
        <td>{{Password}}</td>
        <td><a class="btn btn-success" href="/user/update/{{id}}">Modificar</a><a class="btn btn-danger" href="/user/delete/{{id}}">Eliminar</a></td>
    </tr>
    {{else}}
    <tr>
        <td colspan="4">No hay mas filas</td>
    </tr>
    {{/each}}
</table>
<br>
<form action="../user/procesar" method="post">
    <table border="1">
        <theader>
            <th>Accionn</th>
            <th>Id</th>
            <th>Nombre</th>
            <th>Password</th>
        </theader>
        {{#each users}}
        <tr>
            <td>
                <input type="checkbox" name="usuario" value="{{id}}">
            </td>
            <td>{{Id}}</td>
            <td>{{Usuario}}</td>
            <td>{{Password}}</td>
        </tr>
        {{else}}
        <tr>
            <td colspan="4">No hay mas filas</td>
        </tr>
        {{/each}}
    </table>
    <button class="btn btn-success">Enviar</button>
</form>
<button class="btn btn-success" onclick="window.location.href='home';">Panel de Control</button>